<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="Schemacrawler"
                location="applications||startMenu"/>
  <shortcut
    name="SchemaCrawler Command Line"
    target="C:\Windows\system32\cmd"
    commandLine='/c "set path=%path%;C:\Program Files\schemacrawler"&amp;&amp;cls&amp;&amp; START'
    workingDirectory="$USER_HOME\"
    description="This starts schemacrawler shell session"
    iconFile="$INSTALL_PATH\schemacrawler.ico"
    iconIndex="0"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    applications="yes"
    startMenu="yes"
    startup="yes">
    
